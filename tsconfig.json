{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "baseUrl": ".",
    "target": "ESNext",
    "module": "es2022",
    "moduleResolution": "node",
    "paths": {
      "#site-lib/*": ["./site/_lib/*"],
      "#site-components/*": ["./site/_components/*"],
      "#site-layouts/*": ["./site/_layouts/*"],
      "#site-utils/*": ["./site/_utils/*"]
    }
  },
  "include": ["**/*.js", "**/*.ts"]
}
